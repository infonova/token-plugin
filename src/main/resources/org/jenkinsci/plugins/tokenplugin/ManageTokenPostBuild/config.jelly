<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
  <f:entry title="${%Action}" field="action">
    <f:select/>
  </f:entry>
  <f:entry title="${%force execution of action if started by timer}" field="forceAction">
    <f:checkbox/>
  </f:entry>
  <f:entry title="${%Name of the System}" field="systemName">
    <f:textbox/>
  </f:entry>
  <f:entry title="${%Header Link}" field="headerLink">
    <f:textbox/>
  </f:entry>
</j:jelly>
